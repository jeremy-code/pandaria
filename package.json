{
  "name": "pandaria",
  "description": "Panda CSS and React Aria UI components",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "panda",
    "pandacss",
    "react-aria",
    "react-aria-components",
    "pandaria",
    "pandaria-ui"
  ],
  "license": "MIT",
  "author": "Jeremy Nguyen <hi@jeremy.ng> (https://jeremy.ng)",
  "repository": "github:jeremy-code/pandaria",
  "homepage": "https://github.com/jeremy-code/pandaria#readme",
  "scripts": {
    "prepack": "husky",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier '**/*.{js,jsx,ts,tsx,json}' --write --ignore-path .gitignore"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@pandaria/eslint-config": "workspace:^",
    "@pandaria/tsconfig": "workspace:^",
    "@types/eslint": "^8.56.2",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.1.0+sha256.81a00df816059803e6b5148acf03ce313cad36b7f6e5af6efa040a15981a6ffb",
  "dependenciesMeta": {
    "@ianvs/prettier-plugin-sort-imports@4.1.1": {
      "unplugged": true
    }
  }
}
