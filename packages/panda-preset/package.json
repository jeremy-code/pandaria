{
  "name": "@pandaria/panda-preset",
  "version": "0.0.1",
  "description": "Panda CSS and React Aria UI components",
  "keywords": [
    "panda",
    "pandacss",
    "react-aria",
    "react-aria-components"
  ],
  "license": "MIT",
  "homepage": "https://pandaria.dev",
  "bugs": "https://github.com/jeremy-code/pandaria/issues",
  "repository": "github:jeremy-code/pandaria",
  "author": "Jeremy Nguyen <hi@jeremy.ng> (https://jeremy.ng)",
  "files": [
    "dist",
    "src"
  ],
  "main": "src/index.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": {
    "replace": {
      "main": "dist/index.js",
      "module": "dist/index.mjs",
      "types": "dist/index.d.ts",
      "exports": {
        ".": {
          "types": "dist/index.d.ts",
          "import": "dist/index.mjs",
          "require": "dist/index.js"
        }
      }
    }
  },
  "dependencies": {
    "deepmerge-ts": "^5.1.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.33.0",
    "@pandacss/types": "^0.33.0",
    "@pandaria/eslint-config": "workspace:^",
    "@pandaria/tsconfig": "workspace:^",
    "@types/eslint": "^8.56.5",
    "clean-package": "^2.2.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@pandacss/dev": ">0.22.0",
    "@pandacss/types": ">0.22.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
